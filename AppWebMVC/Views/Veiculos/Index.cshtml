@model IEnumerable<AppWebMVC.Models.Veiculo>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Placa
            </th>
            <th>
                Marca
            </th>
            <th>
                Modelo
            </th>
            <th>
                Versão
            </th>
            <th>
                Ano Fabricação
            </th>
            <th>
                Ano Modelo
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @item.Placa
            </td>
            <td>
                @item.Marca
            </td>
            <td>
                @item.Modelo
            </td>
            <td>
                @item.Versao
            </td>
            <td>
                @item.AnoFabricacao
            </td>
            <td>
                @item.AnoModelo
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
